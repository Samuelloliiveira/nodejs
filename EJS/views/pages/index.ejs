<!DOCTYPE html>
<html lang="pt-BR">

<%- include('../partials/head'); %>

    <body class="container">

        <!-- passando objeto pelo include -->
        <%- include('../partials/header'); %>

            <main>
                <div class="jumbotron">
                    <h1>Node com EJS</h1>
                    <p>Apredendo a trabalhar com arquivos EJS</p>
                    <p><%= subtitle %></p>
                </div>

                <ul>
                    <% qualitys.forEach(function(quality) { %>
                        <li>
                            <strong><%= quality.title %></strong> <%= quality.message %>
                        </li>  
                    <% }) %>
                </ul>

            </main>

            <%- include('../partials/footer'); %>

    </body>
</html>